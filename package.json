{
  "name": "credential-ld-example",
  "version": "1.0.0",
  "description": "Sample code for issuing and verifying a JSON-LD credential with Veramo",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "npx ts-node --esm src/index.ts",
    "update:veramo:next": "yarn add @veramo/core@next @veramo/credential-ld@next @veramo/credential-w3c@next @veramo/did-manager@next @veramo/did-provider-key@next @veramo/did-provider-ethr@next @veramo/did-resolver@next @veramo/key-manager@next @veramo/kms-local@next",
    "update:veramo:latest": "yarn add @veramo/core@latest @veramo/credential-ld@latest @veramo/credential-w3c@latest @veramo/did-manager@latest @veramo/did-provider-key@latest @veramo/did-provider-ethr@latest @veramo/did-resolver@latest @veramo/key-manager@latest @veramo/kms-local@latest"
  },
  "author": "Mircea Nistor <mircea.nistor@mesh.xyz>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/preset-env": "7.22.14",
    "@babel/preset-typescript": "7.22.11",
    "@types/jest": "29.5.4",
    "babel-jest": "29.6.4",
    "jest": "29.6.4",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@semaphore-protocol/group": "^3.11.0",
    "@semaphore-protocol/identity": "^3.11.0",
    "@semaphore-protocol/proof": "^3.11.0",
    "@types/node": "^18.0.0",
    "@veramo/core": "^5.4.1",
    "@veramo/credential-ld": "^5.4.1",
    "@veramo/credential-w3c": "^5.4.1",
    "@veramo/did-manager": "^5.4.1",
    "@veramo/did-provider-ethr": "^5.4.1",
    "@veramo/did-provider-key": "^5.4.1",
    "@veramo/did-provider-web": "^5.4.1",
    "@veramo/did-resolver": "^5.4.1",
    "@veramo/key-manager": "^5.4.1",
    "@veramo/kms-local": "^5.4.1",
    "web-did-resolver": "^2.0.27"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:veramolabs/credential-ld-example.git"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "type": "module"
}
